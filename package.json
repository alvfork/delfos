{
  "name": "delfos",
  "version": "0.0.0-development",
  "description": "Tiny set of assertions, useful for common validations",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --recursive \"src/*$MATCH*/*.test.js\" --require @babel/register",
    "watch:test": "npm t -- -w",
    "cover": "nyc --reporter=lcov npm test",
    "summary:cover": "nyc npm test",
    "semantic-release": "semantic-release",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --ignore '**/*.test.js' --copy-files --no-copy-ignored --no-comments --out-dir dist src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alvfork/delfos.git"
  },
  "keywords": [
    "delfos",
    "assertions",
    "validations"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Alvaro Araujo <alvaro@clustergy.com> (https://clustergy.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvfork/delfos/issues"
  },
  "homepage": "https://alvfork.github.io/delfos/",
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/register": "7.12.10",
    "babel-loader": "8.2.2",
    "babel-preset-minify": "0.5.1",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "documentation": "^13.1.1",
    "docusaurus": "^1.14.6",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-mdx": "1.8.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "1.8.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "4.3.8",
    "json-loader": "0.5.7",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "^17.3.7",
    "webpack": "5.17.0",
    "webpack-cli": "4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npx eslint --ext .js",
      "npm run cover && npm run check-coverage"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "minify",
      "@babel/preset-env"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {}
}
